# Example Prometheus configuration for scraping GitHub Copilot metrics

global:
  scrape_interval: 5m
  evaluation_interval: 5m

scrape_configs:
  - job_name: 'github-copilot-metrics'
    scrape_interval: 5m
    scrape_timeout: 30s
    static_configs:
      - targets: ['localhost:9101']
        labels:
          service: 'github-copilot-exporter'
